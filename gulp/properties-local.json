    "backend-files": [
        "package.json", "package-lock.json", "code-backend/src/main/**"
    ],
    "backend-build-dir": "temp/build/code-backend",
    "frontend-s3-bucket": "leggett.link",
    "frontend-build-dir": "temp/build/code-frontend/root",
